{
	"eslint.run": "onSave",
	"editor.formatOnSave": true,
	"prisma.fileWatcher": true,
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma",
		"editor.formatOnSave": true
	},
	"cSpell.words": ["includepaths", "NAPI", "paren", "Parens", "precommit"],
	"cSpell.ignorePaths": [
		"package-lock.json",
		"node_modules",
		"vscode-extension",
		".git/objects",
		".vscode",
		".vscode-insiders",
		"src/initialDbMigration/dummy/inventory.ts",
		"yarn-error.log",
		"yarn.lock"
	]
}
